<BaseLayout title="Poufs">
    <div class="category-breadcrumbs" slot="pre-main">
        <ul class="breadcrumbs">
            <li class="breadcrumbs__item">
                <a href="/en">Home</a>
                <svg class="icon icon-chevron-right ">
                    <use xmlns:xlink="http://www.w3.org/1999/xlink"
                         xlink:href="/static/std/assets/toolkit/svg/sprite.svg#icon-chevron-right"></use>
                </svg>
            </li>
            <li class="breadcrumbs__item">
                <a href="/en/collection">collection</a>
                <svg class="icon icon-chevron-right ">
                    <use xmlns:xlink="http://www.w3.org/1999/xlink"
                         xlink:href="/static/std/assets/toolkit/svg/sprite.svg#icon-chevron-right"></use>
                </svg>
            </li>
            <li class="breadcrumbs__item">Poufs</li>
        </ul>
    </div>
    <div class="category-header">
        <p class="category-header__category-title">Poufs</p>
        <p class="category-header__category-description">The Otto is a multifunctional pouf. Use it as an extra seat,
            footstool or side table.</p>
    </div>
    <div class="sh-org-modal" data-variant="filter">
        <div class="sh-molecule-bg-box">
            <div class="sh-molecule-padding-box" style="
                        --size-spacing-units-top: 0;
                        --size-spacing-units-right: 5;
                        --size-spacing-units-bottom: 5;
                        --size-spacing-units-left: 10;
                        --size-spacing-units-top-desktop: 0;
                        --size-spacing-units-right-desktop: 5;
                        --size-spacing-units-bottom-desktop: 5;
                        --size-spacing-units-left-desktop: 10;
                    ">
                <div class="sh-molecule-l-button-group" data-alignment-mobile="end" data-alignment-desktop="start">
                    <button
                        type="button"
                        class="sh-atom-button"
                        onclick="orgModal.openModal()"
                        style="--color-background-button: var(--color-accent7); --color-text-button: var(--color-henk-white); --color-border-button: var(--color-accent7);">
                        filter
                    </button>
                </div>

                <div class="sh-org-modal__inner">
                    <div class="sh-org-modal__modal-container">
                        <div class="sh-molecule-bg-box" style="--color-background-component-bgbox:var(--color-White);">
                            <div class="sh-molecule-padding-box" style="
                                --size-spacing-units-top: 7;
                                --size-spacing-units-right: 7;
                                --size-spacing-units-bottom: 7;
                                --size-spacing-units-left: 7;
                                --size-spacing-units-top-desktop: 7;
                                --size-spacing-units-right-desktop: 7;
                                --size-spacing-units-bottom-desktop: 7;
                                --size-spacing-units-left-desktop: 7;
