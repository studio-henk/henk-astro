{
    "extends": "astro/tsconfigs/base",
    // "include": ["types/iconTypes.ts"],
    "include": [
        "src/**/*.ts",
        "src/**/*.d.ts",
        "types/**/*.ts"
    ],
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@components/*": ["src/components/*"],
            "@assets/*": ["src/assets/*"],
            "*": ["types/*"]
        },
    }
}