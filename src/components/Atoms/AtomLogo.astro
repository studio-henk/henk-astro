---
import Icon from "../Icon.astro";
const { url = "/", icon = "henk-logo", width = 174, height = 16 } = Astro.props;
---
<a href={url} class="atom-logo">
    <Icon
        icon={icon}
        width={width}
        height={height}
    />
</a>
<style>
    .atom-logo {
    }

    .atom-logo svg {
        width: 82px;
        height: 24px;
    }
</style>